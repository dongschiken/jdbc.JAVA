<%
	// 삭제할 쿠키의 값들이 전부 들어가진다.
	String[] delCookieNames = request.getParameterValues("ckbCookie");
	for(int i = 0; i < delCookieNames.length; i++){
		String cookieName = delCookieNames[i];
		String cookieValue = "";
		Cookie c = new Cookie(cookieName, cookieValue);
	}


%>
